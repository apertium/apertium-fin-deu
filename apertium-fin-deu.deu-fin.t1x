          </test>
          <let>
            <clip pos="1" side="tl" part="a_case"/><lit-tag v="nom"/>
          </let>
          <let>
            <var n="case"/><lit-tag v="nom"/>
          </let>
        </when>
        <!-- otherwise, same case -->
        <otherwise>
          <let>
            <clip pos="1" side="tl" part="a_case"/><lit-tag v="nom"/>
          </let>
          <let>
            <var n="case"/><lit-tag v="nom"/>
